function str_rand() {
var result = '';
var words = '0123456789qwertyuiopasdfghjklzxcvbnmQWERTYUIOPASDFGHJKLZXCVBNM';
var max_position = words.length - 1;
for( i = 0; i < 10; ++i ) {
position = Math.floor ( Math.random() * max_position );
result = result + words.substring(position, position + 1);
}
return result;
}
function set_pass() {
login = $('#register_login').val();
json_val = 1;
value_pass = str_rand();
value_pass2 = value_pass;
host = location.host;
page_param = "note";
$.ajax({
type: "POST",
url: "http://"+host+"/subscribe/_register",
data: "login="+login+"&password="+value_pass+"&password2="+value_pass2+"&json="+json_val+"&page_param="+page_param,
dataType: 'json',
success: function(items) {
json = items;
if (login == ''){
$('form input[id="register_login"]').css('border','1px solid #671a1d');
return false;
} else if (!(/^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i.test(login))){
$('form input[id="register_login"]').css('border','1px solid #671a1d');
return false;
} else if (json.ok != 0){
$('.subscribe-adfox-down').html('');
$('.subscribe-adfox-down').html('<h2>Спасибо! Мы отправили вам письмо для подтверждения.</h2>');
} else {
$('.subscribe-adfox-down').html('');
$('.subscribe-adfox-down').html('<h2>Вы уже получаете нашу рассылку. Оставайтесь с нами!</h2>');
}
}
});
return false;
}
