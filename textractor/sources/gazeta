<script>
var arr = []; arr.push({url:'/ssi/header_tizer/_7685164.shtml', timer:100}); arr.push({url:'/ssi/header_tizer/_7685182.shtml', timer:100}); arr.push({url:'/ssi/header_tizer/_7685194.shtml', timer:100}); arr.push({url:'/ssi/header_tizer/_7685188.shtml', timer:100}); var r = Math.random();
var ind;
var url;
if (r < 0.25)
ind = 0;
else if (r < 0.5)
ind = 1;
else if (r < 0.75)
ind = 2;
else if (r < 1)
ind = 3;
url = arr[ind].url;
$( "#placeHeaderTizer" ).load( url ); setTimeout( nextHeaderTizer, arr[ind].timer*1000 );
function nextHeaderTizer () {
ind++;
if ( ind >= 4 )
ind = 0;
url = "";
url = arr[ind].url;
if (url){
$( "#placeHeaderTizer" ).load( url );
}
setTimeout( nextHeaderTizer, arr[ind].timer*1000 );
} </script>

